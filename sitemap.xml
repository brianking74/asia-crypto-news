    <script>
        function copyXMLContent() {
            const xmlContent = document.getElementById('xmlContent').textContent;
            
            if (navigator.clipboard && window.isSecureContext) {
                navigator.clipboard.writeText(xmlContent).then(() => {
                    alert('XML sitemap content copied to clipboard! Create a new file called "sitemap.xml" and paste the content.');
                }).catch(err => {
                    console.error('Failed to copy: ', err);
                    fallbackCopy(xmlContent);
                });
            } else {
                fallbackCopy(xmlContent);
            }
        }
        
        function fallbackCopy(text) {
            const textArea = document.createElement('textarea');
            textArea.value = text;
            textArea.style.position = 'fixed';
            textArea.style.left = '-999999px';
            textArea.style.top = '-999999px';
            document.body.appendChild(textArea);
            textArea.focus();
            textArea.select();
            
            try {
                document.execCommand('copy');
                alert('XML sitemap content copied to clipboard! Create a new file called "sitemap.xml" and paste the content.');
            } catch (err) {
                alert('Please manually select and copy the XML content below.');
            }
            
            document.body.removeChild(textArea);
        }
    </script>
